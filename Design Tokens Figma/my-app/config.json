{
  "source": ["tokens/tokens.json"],
  "platforms": {
    "scss": {
      "transformGroup": "scss",
      "buildPath": "src/scss/",
      "files": [{
        "destination": "_variables.scss",
        "format": "scss/variables"
        
      }]
    },
    "json": {
      "transformGroup": "scss",
      "buildPath": "src/scss/",
      "files": [
        {
          "destination": "design-tokens.json",
          "format": "json/nested"
        }
      ]
    }
  }
  
}

